<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobasher">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
    <application
        android:icon="@mipmap/mobasher_icon_fresh"
        android:label="Mobasher مباشر"
        android:roundIcon="@mipmap/mobasher_icon_fresh_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppartmentsActivity"/>
        <activity android:name=".NewPasswordActivity" />
        <activity android:name=".PasswordResetActivity" />
        <activity android:name=".OtpActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>